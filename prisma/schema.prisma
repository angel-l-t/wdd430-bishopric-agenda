generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model tasks {
  id          String @id @default(auto()) @map("_id") @db.ObjectId
  assigned    String
  dateTime    String
  dateCreated String
  description String
  title       String
  type        String
}
